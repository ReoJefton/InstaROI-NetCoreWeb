@model InstaRoi.Models.MarketingElements
@{
    ViewData["Title"] = "Start";
}

@*<h2>Faq</h2>*@

<style>
    .accordion {
        margin-bottom: 10px;
        background-color: #444;
        color: #ffffff;
        cursor: pointer;
        padding: 18px;
        width: 100%;
        text-align: left;
        outline: none;
        font-size: 15px;
        box-shadow: 0 4px 8px 0 rgba(0,0,0,0.2);
        transition: 0.4s;
        border-radius: 5px;
        border: 1px solid rgba(0, 0, 0, 0.125);
        border-color: #444;
        font-family: Segoe UI,Tahoma,Geneva,Verdana,sans-serif !important;
    }

        .active, .accordion:hover {
            box-shadow: 0 8px 16px 0 rgba(89,38,124,5.2);
            background-color: #444;
        }
</style>

<h4>Let's get started</h4>
<button class="accordion w3-animate-left">Select a subscription level to create an account and include payment information here: <a asp-area="" asp-controller="Account" asp-action="Subscribe" class="" style="font-size:15px;">Subscribe</a> </button>

<button class="accordion w3-animate-left">If you are an existing user, login here: <a asp-area="" asp-controller="Account" asp-action="Login" class="" style="font-size:15px;">Login</a></button>

@*<button class="accordion w3-animate-left">If you are an Administrator of the account, you can assign up to 2 additional users on the account each year or 4 additional users if you sign-up on the annual plan</button>*@

<button class="accordion w3-animate-left">Start calculating your ROI by clicking here: <a asp-area="" asp-controller="Home" asp-action="Index" class="" style="font-size:15px;">Marketing Elements</a></button>

@*<button class="accordion w3-animate-left">The purchase includes a custom tutorial</button>*@
